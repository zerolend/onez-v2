{
  "name": "onez-v2",
  "description": "A yield bearing decentralized stablecoin on zkSync",
  "private": true,
  "author": "ZeroLend",
  "license": "MIT",
  "scripts": {
    "deploy:hardhat": "hardhat --config hardhat.config.ts run scripts/deploy-hardhat-local.ts",
    "deploy:base:goerli": "hardhat --config hardhat.config.ts run scripts/deploy-hardhat-local.ts --network baseGoerli",
    "deploy:zksync:goerli-fork": "hardhat --config hardhat.config.zksync.ts deploy-zksync --script zksync_goerli_fork.ts",
    "deploy:zksync:goerli": "hardhat --config hardhat.config.zksync.ts deploy-zksync --script zksync_goerli.ts --network zkSyncTestnet",
    "deploy:zksync:mainnet-fork": "hardhat --config hardhat.config.zksync.ts deploy-zksync --script zksync_mainnet_fork.ts",
    "deploy:manta:testnet": "hardhat --config hardhat.config.ts run scripts/deploy-manta-testnet.ts --network manta-testnet",
    "hardhat:local:run": "hardhat --config hardhat.config.ts run",
    "deploy-zk": "hardhat --config hardhat.config.zksync.ts deploy-zksync --script deploy.ts",
    "compile": "hardhat compile",
    "compile-zk": "hardhat --config hardhat.config.zksync.ts compile",
    "clean": "rimraf abis typechain artifacts artifacts-zk cache cache-zk",
    "test": "hardhat test --network hardhat"
  },
  "devDependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.1",
    "@layerzerolabs/solidity-examples": "git+https://github.com/LayerZero-Labs/solidity-examples.git#3d3a09f14a1d05479a5a397d5f646fe3a455c00c",
    "@matterlabs/hardhat-zksync-deploy": "^0.6.5",
    "@matterlabs/hardhat-zksync-node": "^0.0.1-beta.6",
    "@matterlabs/hardhat-zksync-solc": "^0.4.2",
    "@matterlabs/hardhat-zksync-verify": "^0.2.1",
    "@matterlabs/zksync-contracts": "^0.6.1",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.9",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.1",
    "@openzeppelin/contracts": "4.8.0",
    "@pythnetwork/pyth-sdk-solidity": "^2.3.0",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/bluebird": "^3.5.42",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "bluebird": "^3.7.2",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.4",
    "hardhat-abi-exporter": "^2.10.1",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.5",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^4.9.5",
    "zksync-web3": "^0.14.3"
  },
  "dependencies": {
    "prettier-plugin-solidity": "^1.2.0"
  }
}
